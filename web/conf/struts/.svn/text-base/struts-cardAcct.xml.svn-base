		</action>
		<action name="listDiscontinuousCard" class="gnete.card.web.cardInfo.CardAcctReviewAction" method="listDiscontinuousCard">
			<result name="listDiscontinuousCard">listDiscontinuousCard.jsp</result>
		</action>
		<action name="acctTransList" class="gnete.card.web.cardInfo.CardAcctReviewAction" method="acctTransList">
			<result name="acctTransList">acctTransList.jsp</result>
		</action>
		<action name="detail" class="gnete.card.web.cardInfo.CardAcctReviewAction" method="detail">
			<result name="detail">detail.jsp</result>
