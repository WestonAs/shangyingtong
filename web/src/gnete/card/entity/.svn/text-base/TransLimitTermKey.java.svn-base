package gnete.card.entity;

public class TransLimitTermKey {

	private String	cardIssuer;

	private String	cardBin;

	private String	merNo;

	private String	termNo;

	public TransLimitTermKey() {
		super();
	}

	public TransLimitTermKey(String cardIssuer, String cardBin, String merNo, String termNo) {
		super();
		this.cardIssuer = cardIssuer;
		this.cardBin = cardBin;
		this.merNo = merNo;
		this.termNo = termNo;
	}

	@Override
	public String toString() {
		return "TransLimitTermKey [" + (cardBin != null ? "cardBin=" + cardBin + ", " : "")
				+ (cardIssuer != null ? "cardIssuer=" + cardIssuer + ", " : "")
				+ (merNo != null ? "merNo=" + merNo + ", " : "") + (termNo != null ? "termNo=" + termNo : "")
				+ "]";
	}

	// ------------------------------- getter and setter followed------------------------

	public String getCardIssuer() {
		return cardIssuer;
	}

	public void setCardIssuer(String cardIssuer) {
		this.cardIssuer = cardIssuer;
	}

	public String getCardBin() {
		return cardBin;
	}

	public void setCardBin(String cardBin) {
		this.cardBin = cardBin;
	}

	public String getMerNo() {
		return merNo;
	}

	public void setMerNo(String merNo) {
		this.merNo = merNo;
	}

	public String getTermNo() {
		return termNo;
	}

	public void setTermNo(String termNo) {
		this.termNo = termNo;
	}
}